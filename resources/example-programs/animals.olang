USING [
    CLASS Animal(INT id)
    FIELDS INT id
    INIT {
        this.id := id
    }
    [
        METHOD print() {
            PRINTS "I am an animal with id "
            PRINTI this.id
            PRINTLNS "!"
        }
    ]

    CLASS Horse(INT id)
    SUBCLASSOF Animal
    INIT {
        this.id := id
    }
    [
        METHOD print() {
            PRINTS "I am a horse with id "
            PRINTI this.id
            PRINTLNS "!"
        }
    ]
]
DO {
    OBJ Animal one
    one := Animal(1)
    CALL one.print()

    OBJ Animal two
    two := Horse(2)
    CALL two.print()   
}